{
  "name": "elkhawaga-qr",
  "version": "1.0.0",
  "description": "QR Scanner Worker for Elkhawaga Products",
  "main": "src/index.js",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "deploy:prod": "wrangler deploy --env production",
    "staging:dev": "wrangler dev --config wrangler-staging.toml --remote",
    "staging:dev:local": "wrangler dev --config wrangler-staging.toml",
    "staging:deploy": "wrangler deploy --config wrangler-staging.toml",
    "tail": "wrangler tail",
    "tail:staging": "wrangler tail --config wrangler-staging.toml"
  },
  "keywords": ["cloudflare", "worker", "qr", "elkhawaga"],
  "author": "Elkhawaga",
  "license": "MIT",
  "devDependencies": {
    "wrangler": "^3.80.0"
  }
}
